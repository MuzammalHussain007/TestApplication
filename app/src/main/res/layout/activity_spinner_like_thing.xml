<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".New.SpinnerLikeThing">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_16sdp"
        android:padding="@dimen/_5sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">





    </androidx.constraintlayout.widget.ConstraintLayout>

 <!--   android:layout_width="@dimen/_120sdp"-->
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/manualLocation"
        android:layout_width="@dimen/_120sdp"
        android:padding="@dimen/_7sdp"
        android:background="@drawable/curved_background_spinner"
        android:layout_height="@dimen/_30sdp"
        android:text="Manual Location"
        android:gravity="center"
        android:textColor="@color/yellow"
        android:textSize="@dimen/_12ssp"
        app:fontFamily="@font/inter_med"
        app:drawableTint="@color/white"
        android:drawableEnd="@drawable/ic_not_collapse"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />






</androidx.constraintlayout.widget.ConstraintLayout>